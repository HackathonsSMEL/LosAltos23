{
    "manifest_version": 3,
    "name": "Read Better",
    "description": "Uses ML to highlight important parts of passages",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
    "js": ["js/main.js"]
      }
],
    "action": {
      "default_popup": "popup/popup.html"

      
    },
    "icons": {
      "128": "highlighticon.png"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
  ]
  }
  